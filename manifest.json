{
  "manifest_version": 3,
  "name": "My Local Notepad WITH FOLDERS",
  "description": "Simple local notepad that saves automatically, WITH FOLDERS",
  "version": "1.0.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon16.png",
      "48": "icons/icon16.png",
      "128": "icons/icon16.png"
    }
  },
  "permissions": ["storage"]
}
